<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<table>
    <thead>
        <th>Images</th>
        <th>text</th>
        <th>likes</th>
        <th>comments</th>
        <th></th>
    </thead>
    <tbody>
    <% @posts.each do |post| %>
        <tr>
            <td><img src="<%= post.image_url %>"></td>
            <td><%= post.text %></td>
            <td><%= post.likes %></td>
            <td><%= post.comments %></td>
            <td><a href="<%=  post_path(post.id)%>">Mostrar detalle</a></td>
            <td><a href="<%=  edit_post_path(post.id)%>">editar post</a></td>
        </tr>
    <% end %>
    </tbody>
</table>
<a href="<%= root_path %>">Volver al inicio</a>
<a href="<%= new_post_path %>">Crear nuevo post</a>